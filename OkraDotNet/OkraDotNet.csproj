<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Abiola Olakunle</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.1.0</Version>
    <Description>Okra.ng API for .Net Projects</Description>
    <Copyright>Copyright 2020</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/abiolakunle/OkraDotNet</PackageProjectUrl>
    <RepositoryUrl>https://github.com/abiolakunle/OkraDotNet.git</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Okra.ng Bank account connection</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.1.7" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="CSharpFunctionalExtensions" Version="2.11.6" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Balance\Responses\Common\" />
  </ItemGroup>

</Project>
